{
  "name": "@platforma-open/milaboratories.mixcr-clonotyping-2.software",
  "version": "1.0.1",
  "scripts": {
    "build": "pl-pkg build",
    "prepublishOnly": "pl-pkg publish packages",
    "do-pack": "rm -f *.tgz && pl-pkg build && pnpm pack && mv platforma-open*.tgz package.tgz",
    "changeset": "changeset",
    "version-packages": "changeset version"
  },
  "files": [
    "./dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@platforma-sdk/package-builder": "catalog:",
    "@platforma-open/milaboratories.runenv-python-3": "catalog:"
  },
  "block-software": {
    "entrypoints": {
      "preprocessing": {
        "binary": {
          "artifact": {
            "type": "python",
            "registry": "platforma-open",
            "environment": "@platforma-open/milaboratories.runenv-python-3:3.12.6",
            "dependencies": {
              "toolset": "pip",
              "requirements": "requirements.txt"
            },
            "root": "./src/preprocessing"
          },
          "cmd": [
            "python",
            "{pkg}/preprocessing.py"
          ]
        }
      },
      "sc-group-builder": {
        "binary": {
          "artifact": {
            "type": "python",
            "registry": "platforma-open",
            "environment": "@platforma-open/milaboratories.runenv-python-3:3.12.6",
            "dependencies": {
              "toolset": "pip",
              "requirements": "requirements.txt"
            },
            "root": "./src/sc-group-builder"
          },
          "cmd": [
            "python",
            "{pkg}/sc-group-builder.py"
          ]
        }
      },
      "output_processing": {
        "binary": {
          "artifact": {
            "type": "python",
            "registry": "platforma-open",
            "environment": "@platforma-open/milaboratories.runenv-python-3:3.12.6",
            "dependencies": {
              "toolset": "pip",
              "requirements": "requirements.txt"
            },
            "root": "./src/output_processing"
          },
          "cmd": [
            "python",
            "{pkg}/output_processing.py"
          ]
        }
      }
    }
  }
}
